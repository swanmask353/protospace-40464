<%= link_to prototype_path(prototype.id)  do %>
 <div class="card">
   <%= image_tag(prototype.image, class: 'card__img') %>
   <div class="card__body">
     <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title%>
     <p class="card__summary">
       <%= prototype.catch_copy %>
    </p>
    <%= link_to current_user.name, root_path, class: :card__user %>
  </div>
 </div>
<% end %>